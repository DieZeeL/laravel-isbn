{
  "name": "diezeel/laravel-isbn",
  "description": "A laravel isbn",
  "license": "MIT",
  "keywords": [
    "laravel",
    "isbn"
  ],
  "type": "library",
  "authors": [
    {
      "name": "DieZ",
      "email": "diez.eel@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "nicebooks/isbn": "^0.2"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Diezeel\\LaravelIsbn\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Diezeel\\LaravelIsbn\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Diezeel\\LaravelIsbn\\LaravelIsbnServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
